<!DOCTYPE hibernate-mapping PUBLIC  "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
<class name = "com.model.GalleryImage" table = "pictures">
<id name = "name" type="java.lang.String" column ="NAME"/>
<property name="directory" type ="java.lang.String" column ="DIRECTORY"/>
<property name="dateTaken" type ="java.sql.Timestamp" column ="DATE_TAKEN"/>

</class>
</hibernate-mapping>