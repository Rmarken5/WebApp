<!DOCTYPE hibernate-mapping PUBLIC  "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
<class name = "com.model.GalleryImage" table = "pictures">

<composite-id>
<key-property name="id" column="id" type = "long"  />
<key-property name="name" column="NAME" type="java.lang.String" />
</composite-id>
<property name="directory" type ="java.lang.String" column ="DIRECTORY"/>
<property name="dateTaken" type ="java.sql.Timestamp" column ="DATE_TAKEN"/>

</class>
</hibernate-mapping>